             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RestDemo.JsonParsing">
    <ListView x:Name="PizzaList">
        <ListView.ItemTemplate>
            <DataTemplate>
                <TextCell Text="{Binding Name}" Detail="{Binding Zip}"/>
            </DataTemplate>
        </ListView.ItemTemplate>
    </ListView>
</ContentPage>